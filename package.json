  "description": "A SPA that tracks user message logs",
  "scripts": {
    "start": "rm -rf .parcel-cache && node server.js",
    "prestart": "rm -rf .parcel-cache && parcel build ./public/index.html ./static/**",
    "dev": "rm -rf .parcel-cache && parcel serve ./public/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "lhuy0594-tracker"
  },
  "author": "Lily Huynh",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "sass": "^1.75.0"
  },
  "staticFiles": {
    "staticPath": ["./static"],
    "watcherGlob": "**/*.xml"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "parcel": "^2.12.0",
    "parcel-plugin-static-files-copy": "^2.6.0"
  }
